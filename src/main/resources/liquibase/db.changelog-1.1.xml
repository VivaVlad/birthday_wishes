<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="liquibase-docs" id="addAutoIncrement-wishes">
        <addAutoIncrement columnDataType="int"
                          columnName="id"
                          incrementBy="1"
                          startWith="1"
                          tableName="wishes"/>
    </changeSet>
    <changeSet author="liquibase-docs" id="addAutoIncrement-users">
        <addAutoIncrement columnDataType="int"
                          columnName="id"
                          incrementBy="1"
                          startWith="1"
                          tableName="users"/>
    </changeSet>
    <changeSet id="tag-1.1" author="vlad">
        <tagDatabase tag="1.1" />
    </changeSet>

</databaseChangeLog>